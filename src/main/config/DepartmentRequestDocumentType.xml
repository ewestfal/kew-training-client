<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="ns:workflow resource:WorkflowData">
	<documentTypes xmlns="ns:workflow/DocumentType"
		xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
		<documentType>
			<name>DepartmentRequest</name>
			<label>Department Request</label>
			<docHandler>http://localhost:8081/docHandler</docHandler>
			<attributes>
              <attribute>
                <name>DepartmentCodeSearchableAttribute</name>
              </attribute>
            </attributes>
			<emailStylesheet>DepartmentRequest.EmailStyle</emailStylesheet>
			<applicationId>kew-training-client</applicationId>
			<routePaths>
				<routePath>
					<start name="Start" nextNode="DepartmentPreApproval" />
					<requests name="DepartmentPreApproval" nextNode="DepartmentApproval" />
					<requests name="DepartmentApproval" nextNode="DepartmentHeadApproval" />
					<requests name="DepartmentHeadApproval" nextNode="RequestManagerApproval" />
					<role name="RequestManagerApproval" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="Start">
					<activationType>P</activationType>
				</start>
				<requests name="DepartmentPreApproval">
					<activationType>R</activationType>
					<peopleFlows>
						<peopleFlow>
							<actionRequested>A</actionRequested>
							<name namespace="KEW-TRAINING-CLIENT">DepartmentPreApproval</name>
						</peopleFlow>
					</peopleFlows>
				</requests>
				<requests name="DepartmentApproval">
					<ruleTemplate>DepartmentTemplate</ruleTemplate>
				</requests>
				<requests name="DepartmentHeadApproval">
					<ruleTemplate>DepartmentHeadTemplate</ruleTemplate>
				</requests>
				<role name="RequestManagerApproval">
					<activationType>P</activationType>
				</role>
			</routeNodes>
		</documentType>
	</documentTypes>
</data>